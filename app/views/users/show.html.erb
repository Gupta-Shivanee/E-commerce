<% if current_user %>
  <div class = "mb-3">
    <%= "Hello #{current_user.full_name}" %>
  </div>
  <div class = "mb-2">
    <%= "You are now logged in" %>
  </div>
  <div class = "mb-2">
  <%= "Email:- #{current_user.email}" %>
  </div>
  <%= "Contact No:- #{current_user.mobile_no}" %>
  <%= button_to 'Logout', session_path(current_user.id), method: :delete %> 

